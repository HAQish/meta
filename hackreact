var descriptionString = "Should be an array of tuples, length of three, with the format of string:number";

function assert(expectedBehavior, descriptionString) {
  if (expectedBehavior === false) {
    console.log(descriptionString);
  } else {
    console.log("Test Passed");
  }
}

// [["name", monthlyCapacity], ["name2", monthly2]]
// [[factories[0]."factory name", factories[0]."monthly capacity"]]
function getFactoryCapacities(factories) {
  var arr = [];
  for (let i = 0; i < factories.length; i++) {
    arr.push(factories[i]["factory name"], factories[i]["monthly capacity"]);
  }
  return arr;
}

function expectedBehavior(getFactoryCapacities) {
     return arr[0].length === 2 && arr.length === 3 && arr[0][0].typeOf === string && arr[0][1].typeOf === number;
}

assert(expectedBehavior, descriptionString);


